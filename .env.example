# Example environment configuration for HydroBot
# Copy this file to .env and fill in your own values

API_KEY_BINANCE=your_binance_api_key
API_SECRET_BINANCE=your_binance_secret
API_KEY_KRAKEN=your_kraken_api_key
API_SECRET_KRAKEN=your_kraken_secret

# Redis connection URI
REDIS_URL=redis://localhost:6379/0

# State persistence interval in seconds
STATE_PERSIST_INTERVAL=30

LOG_LEVEL=INFO
